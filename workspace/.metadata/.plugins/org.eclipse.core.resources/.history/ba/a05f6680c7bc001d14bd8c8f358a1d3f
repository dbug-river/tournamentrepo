package com.rivertech.controller;


@RestController
@RequestMapping("/api/users")
public class UserController {
	
	@GetMapping
    public String findAll() {
		
		DatawarehouseRepository repo = new DatawarehouseRepository(
				"jdbc:clickhouse://127.0.0.1:8123",
				"default",
				"omar123");
		repo.UpdateFactTable();
		return "Hello";
    }
}
