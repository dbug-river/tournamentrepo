package com.rivertech.model;

import java.util.List;

public class TimeslotGame {

	/***
	 * Game ID
	 */
	private String gameId;
	
	/***
	 * List of players IDs that are entitled to play the game.
	 */
	private List<String> playersId;
	
	
	/***
	 * Constructor for the Game class.
	 * @param gameId The game ID.
	 * @param playersId The players that will play the game.
	 */
	public TimeslotGame(String gameId, List<String> playersId) {
		super();
		this.gameId = gameId;
		this.playersId = playersId;
	}

	public List<String> getPlayersId() {
		return playersId;
	}

	public String getGameId() {
		return gameId;
	}
	
		
}
